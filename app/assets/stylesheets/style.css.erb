body{
  margin: 0px;
  background-color:#f2dd72;
  /*	background-image: url(<%= asset_path 'stripe.png' %>);	*/
  font-family: Trebuchet MS, Arial, sans-serif;
  height:100%;
  margin-top:0px;
text-align: justify;
}
.top{
  position:relative;
  text-align:right;
  margin: auto;
  width:100%;
  background-color:#eabd40;
  height:50px;
  z-index:1;
}

.wrapper{
  position:relative;
  margin: 0px auto;
  width:990px;
  height:100%;
  top:30px;
}
a{
  text-decoration:none;
  color:#157272;
  font-weight:800;
}
a:hover, a:active {
  color:#01a8a8;
}
section{

  background-color: #FFFFFF;
  padding:10px;
  padding-top:50px;
  position:relative;
  margin-top:320px;
  width:100%;
  min-height:530px;
  border-bottom-left-radius:15px;
  border-bottom-right-radius:15px;
}
img.ornament{
  position:absolute;
  top:-15px;
  left:0px;
}
img.logo{
  position:relative;
  z-index:2;
  margin-top:-40px;
}
img.localisation{
  position:absolute;
  right:20px;
  top:-20px;
}
img.button{
  float:right;
  margin-right: -305px;
  margin-top:80px;
}
img.buttontop{
  margin-top:-2px;
  float: right;
}
ul.nav{
  display:block;
  list-style:none;
  position:relative;
  left:320px;
  margin-top:-350px;
}
ul.nav li{
  min-width:140px;
  height:200px;
  margin-right:6px;
  display:block;
  float:left;
  border-bottom-left-radius:20px;
  border-bottom-right-radius:20px;
  border: 2px dashed #f2dd72;
  border-top:none;
}
ul.nav li:hover { 
  background-color:#eabd40;
  border: 2px dashed #69a798 ;
  border-top:none;
}
li.wider{
  width:170px;
}
img.nav{
  margin: auto;
  position:absolute;
  bottom:-200px;
  margin-left:10px;
}

li.error{
  color:#c4273c;
  list-style:none;
  
}

footer{
  background-color: #e2213c;
  height:70px;
  width:100%;
  padding-top:30px;
  text-align:center;
  position:relative;
  bottom:0px;
  margin-top:40px;
}
.lokal{
  margin-bottom:20px;
  margin-left:100px;
  display:block;
  width:500px;
  height:200px;
}
.lokal_info{
  display:inline-block;
  float:left;
  margin-left:20px;
}
img.lokal_min{
float:left;
}
.form_rejestr, .form_new_local, .form_login{
  position:relative;
  padding:10px;
  margin-top:10px;
  border-top-left-radius:10px;
  border-top-right-radius:10px;
}

.form_rejestr{
  background-color:#c4d37a;
  width:400px;
  margin-left:280px;
}
.form_login{
  width: 350px;
  margin:auto;
}
.field_login{
  border:3px solid #69a798;
  background-color: #a2dbcf;
  width:300px;
}
.field_login_inline{
  width:150px;
  height:15px;
}
.field_search{
  border: 3px solid #7fcbe3;
  margin-top:20px;
  margin-left:60px;
  width:100px;
}
.form_login_inline{
  display:inline-block;
  margin-top:10px;
}

.form_new_local{
  position:relative;
  width:500px;
  margin: auto;
}
.form_add_theory{
  margin:auto;
  margin-top:-30px;
  width:506px;
  background-image: url(<%= asset_path 'square.png' %>);	
}
.field, .area{
  border:3px solid #c4d27a;
  width:400px;
  background-color:#e3edad;
  
}
.area{
  height:150px;
  border-radius:10px;
  padding:5px;
}
.transparent{
  background-color:transparent;
  width:450px;
  border: 1px solid #000000;
  margin-left:23px;
  border-radius:10px;

  margin-bottom:-20px;
}

.comments{
  width:400px;
  margin:auto;
}
.comment{
  width: 350px;
  min-height:20px;
  padding:10px;
  background-color:#e0f1df;
    margin-bottom:10px;
  border-radius:10px;
}
.date{
  font-size:12px;
 margin-left:10px;
}
input:focus, input:hover, textarea:focus, textarea:hover{
  background-color:#fff;
}

input{
  height:20px;
  padding:5px;
  border:none;
  border-radius:5px;
}
input.zarejestruj{
  height:93px;
  width:420px;
  background-image: url(<%= asset_path 'zarejestruj.png' %>);	
  background-color:transparent;
  border-radius:5px;
  border: 0;
  cursor:pointer;
  cursor:hand;
  position:absolute;
  bottom:-90px;
  left:0px;

}
input.add_local{
  background-image: url(<%= asset_path 'dodajprzycisk.png' %>);	
  width:420px;
  height:80px;
  cursor:pointer;
  cursor:hand;
  border:0;
  background-color:transparent;
}
input.login, input.login_small{
  cursor:pointer;
  cursor:hand;
  background-color:transparent;
}
input.login{
   background-image: url(<%= asset_path 'buttonloginbigger.png' %>);
  width:328px;
  height:86px;
}
input.login_small{
  background-image: url(<%= asset_path 'buttonlogin.png' %>);
 background-position: center bottom;
  width:193px;
  height:46px;
  display:inline;
  position:relative;
  margin-top:3px;
}
input.add_comment{

  background-image: url(<%= asset_path 'buttoncomment.png' %>);
  width:175px;
  height:80px;
  background-color:transparent;
  margin-left:250px;
  cursor:pointer;
  cursor:hand;
}
input.add_theory{
  background-image: url(<%= asset_path 'buttontheory.png' %>);
  margin-left:190px;
  width:125px;
  height:80px;
  background-color:transparent;
  cursor:pointer;
  cursor:hand;
}

input.zarejestruj:hover{

}
img.drpancake{
  float:right;
  right:20px;
  z-index:1;
  position:absolute;
}
.tableofcontents{
float:left;
}
input.button_search{
  background-color:transparent;
  background-image: url(<%= asset_path 'buttonsearch.png' %>);
  float:right;
  width:125px;
  height:80px;
  cursor:pointer;
  cursor:hand;
}
.search{
  width:310px;
  float:right;
}

.cloud{
  border: 3px solid #7ecae2;
  min-height:300px;
  width:400px;
  margin-left:141px;
  margin-bottom:-10px;
  padding:10px;
  border-radius:20px;
}
.titles{
  text-align:center;
  font-family: georgia;
}

.titles2{
  font-family:georgia;
  padding:2px 5px;
  border-left:5px solid #84c8df;
  margin-bottom:3px;
  font-size:12px;
  text-align:left;
}
.tableofcontents{
  width:120px;
  min-height:100px;
  margin-top:20px;
}
.undercloud{
  width:400px;
  margin-left:141px;
  margin-top:5px;
  height:80px;
}
.option{
  min-width:40px;
  padding:3px 10px;
  
  border-radius:5px;
  background-color:#91bdc4;
  float:right;
  margin-left:10px;
  margin-top:15px;
}
img.newtheory{
  float:left;
}

.pagination{
  padding: 3px;
  margin:auto;
   min-width:50px;
   border-top: 2px solid #f2dd72;
   text-align:center;
   margin-bottom:40px;
}

.pagination a {
  color: #eabd40;

}
.pagination a:hover, .pagination a:active{
  color: #d39117;
}
